<div class="well">
  <fieldset>
    <legend>Tower Search</legend>
    <%= form_tag('/cell_towers/fetch_cell_tower') do -%>
      <div class="field">
        <%= autocomplete_field_tag :cell_id, '', autocomplete_cell_tower_cell_id_cell_towers_path, :size => 35, :placeholder => "Enter Cell-ID" %>
      </div>
      
      <div><%= submit_tag 'Search', class: 'btn' %></div>
    <% end -%>
  </fieldset>
</div>

