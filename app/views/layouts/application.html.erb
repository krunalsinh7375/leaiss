<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Data Analysis<%#= full_title(yield(:title)) %></title>

    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%= stylesheet_link_tag    "application", media: "all" %>
    <%= stylesheet_link_tag    "bootstrap", media: "all" %>
    <%= stylesheet_link_tag    "docs", media: "all" %>
    <%= stylesheet_link_tag    "layout", media: "all" %>
    <%= stylesheet_link_tag    "bootstrap-responsive", media: "all" %>
    
    <%#= stylesheet_link_tag    "bootstrap.css", media: "all" %>
    <%#= stylesheet_link_tag    "bootstrap-responsive.css", media: "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    <%= render 'layouts/shim' %>    
  </head>
  <body>
    <%= yield :head %>
    <%= render 'layouts/header' %>
    <div class="container">
      <%= yield %>
      <%= render 'layouts/footer' %>
    </div>
    <%= yield :scripts %>
  </body>
</html>