<div class="well">
  <%= form_tag sessions_path do %>
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email], placeholder: 'Email', autofocus: true, class: 'span3' %>

    <%= label_tag :password, "Password" %>
      <%= password_field_tag :password, nil, placeholder: 'Password', class: 'span3' %>
    <br/>
    <%= submit_tag " Login ", class: 'btn'%>
  <% end %>
</div>